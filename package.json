{
    "name": "blogging-mern-app",
    "version": "1.0.0",
    "description": "Blogging app where user can post blogs.",
    "main": "index.js",
    "scripts": {
        "install-server": "cd server && npm install",
        "start-server": "cd server && node index.js",
        "install-client": "cd client && npm install",
        "build-client": "cd client && ng build --prod",
        "start-client": "cd client && ng serve --open",
        "start": "concurrently \"npm run start-server\" \"npm run start-client\""
    },
    "keywords": [
        "blog"
    ],
    "author": "Shrikant Ghuge",
    "license": "ISC",
    "devDependencies": {
        "@angular/cli": "^17.0.0",
        "concurrently": "^8.2.2"
    }
}